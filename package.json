{
  "name": "@oran9e/three-mcmodel",
  "version": "0.0.8",
  "description": "Minecraft model loader for threejs",
  "homepage": "https://github.com/OrangeUtan/three-mcmodel",
  "bugs": {
    "url": "https://github.com/OrangeUtan/three-mcmodel/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OrangeUtan/three-mcmodel.git"
  },
  "license": "MIT",
  "author": "Oran9eUtan <Oran9eUtan@gmail.com>",
  "keywords": [
    "minecraft",
    "minecraft-model",
    "resourcepack",
    "threejs",
    "3d",
    "threejs-loader",
    "three"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "npm run clean && npm run compile",
    "ava": "ava -v",
    "clean": "rimraf --no-glob dist",
    "compile": "tsc",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "docs": "docs-ts",
    "lint": "eslint --ext ts . package.json",
    "lint:fix": "eslint --ext ts --fix . package.json",
    "lint-staged": "lint-staged",
    "prettier": "prettier --list-different src/**/*.ts test/**/*.ts",
    "prettier:fix": "prettier --write src/**/*.ts test/**/*.ts",
    "test": "nyc ava && npm run docs",
    "watch:ava": "nodemon -e \"ts json\" -x \"yarn ava -- -s -v --fail-fast || true\"",
    "watch:compile": "nodemon -e \"ts json\" -x \"yarn compile || true\"",
    "watch:lint": "nodemon -e \"ts js json\" -x \"yarn lint || true\"",
    "watch:test": "nodemon -e \"ts json\" -x \"yarn test || true\""
  },
  "dependencies": {
    "@oran9e/minecraft-model": "^1.2.0",
    "three": "^0.127.0"
  },
  "devDependencies": {
    "@ericcrosson/eslint-config": "^1.1.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.1",
    "@semantic-release/npm": "^7.1.1",
    "@semantic-release/release-notes-generator": "^9.0.2",
    "@types/node": "^14.14.17",
    "@types/three": "^0.127.1",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "ava": "^3.14.0",
    "ava-fast-check": "^4.0.0",
    "codecov": "^3.8.1",
    "docs-ts": "^0.5.3",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-plugin-ava": "^10.2.0",
    "eslint-plugin-fp-ts": "^0.2.1",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-security": "^1.4.0",
    "fast-check": "^2.10.0",
    "lint-staged": "^10.5.3",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "2.2.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.4.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "!dist/**/*.d.ts",
      "test/**/test-*.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/*.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
